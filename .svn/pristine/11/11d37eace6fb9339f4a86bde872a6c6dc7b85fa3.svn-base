// 弹窗内容区样式
.model_table{
  padding: 5px;
  overflow: auto;
  box-sizing: border-box;
  &.table_500{
    height: 500px;
  }
  &.table_550{
    height: 550px;
  }
  table{
    margin: 0;
    width: 100%;
    min-width: auto;
    tr.odd>td{
      margin: 0;
      padding: 4px;
      vertical-align: middle;
      &.pointer{
        cursor: pointer;
      }
    }
    .info_none{
      td{
        text-align: center;
      }
    }
    .info_page{
      td{
        padding: 6px;
      }
    }
  }
}
