.el-dialog{
	border:  2px solid rgba(96,96,96,1);
}
.el-dialog .el-dialog__body{
	padding: 0;
}
.el-dialog .el-dialog__header{
  position: relative;
  padding: 5px 8px;
	background: #F2F2F2;
  margin-bottom: 5px;
  color: #000000;
  &::before{
    content: 'Esc Key';
    position: absolute;
    font-size: 12px;
    top: -2px;
    right: 57px;
    color: #000;
  }
}
.el-dialog .el-dialog__headerbtn{
  background: url("../../../static/close.jpg")no-repeat;
  width: 43px;
	top: 0px;
  right: 8px;
  i{
    &::before{
      content: '';
    }
  }
}
.el-dialog .el-dialog__title{
	font-size: 13px;
  color: #333;
  font-weight: 700;
}
