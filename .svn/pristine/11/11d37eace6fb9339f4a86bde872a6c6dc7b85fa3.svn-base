// 表单类
.form_container{
  height: 500px;
  padding: 0 10px;
  overflow: auto;
  box-sizing: border-box;
// 头部
.formBox_title{
    height: 25px;
    line-height: 25px;
    background: url(../../../static/images/new/headtd.jpg) repeat-x left center;
    // background: -webkit-linear-gradient(left, #2DA4DD, #0E8CC7);
    // background: linear-gradient(to right, #2DA4DD, #0E8CC7);
    font-size: 13px;
    color: #fff;
    padding-left: 5px;
    font-weight: bold;
  }
  ul{
    border: 1px solid #ccc;
    border-bottom: 0;
    li{
      display: flex;
      align-items: center;
      border-bottom: 1px solid #ccc;
      padding: 4px 0;
      color: #000;
      input,select{
        height: 28px;
        line-height: 28px;
        padding:0 10px;
      }
      .formLi_title{
        width: 110px;
        padding-left: 5px;
        font-weight: bold;
        font-size: 14px;
      }
      .formLi_content{
        width: 300px;
        margin-right: 20px;
        input{
          width: 100%;
        }
      }
      .formLi_hint{
        font-size: 12px;
      }
      // 提交按钮
      .form_commit{
        height: 30px;
        line-height: 30px;
        font-size: 14px;
        padding: 0 20px;
        text-align: center;
        // background: #EAEAEA;
        background: -webkit-linear-gradient(left, #efefef, #eee);
        background: linear-gradient(to right, #efefef, #eee);
        border: 1px solid #aaa;
        margin: 2px auto;
      }
    }
  }
}
