// 按钮
.commit_btn{
	padding: 2px 10px;
	cursor: pointer;
}
// 原生按钮样式
a.buttonCss,.buttonCss{
  padding: 4px;
  line-height: 20px;
  color: buttontext;
  white-space: nowrap;
  font-size: 13px;
  -webkit-appearance: button;
}
a.buttonCss:link,a.buttonCss:visited{
  color: buttontext;
}
a.buttonCss:hover{
  text-decoration: none;
}
.inline-flex{
  display: inline-flex;
  align-items: center;
}

