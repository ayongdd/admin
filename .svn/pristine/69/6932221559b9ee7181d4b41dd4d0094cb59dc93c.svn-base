/* 问号提示 */
.help-tip{
	position: relative;
	text-align: center;
	background-color: #20B2AA;
	border-radius: 50%;
	top: 6px; right: 0px;
	width: 12px;
	height: 12px;
	font-size: 12px;
	line-height: 12px;
	float:right;
}
.help-tip:before{
	content:'?';
	font-weight: bold;
	color:#fff;
}
.help-tip:hover p{
	display:block;
}
.help-tip p{
	display: none;
	text-align: left;
	background-color: #F8F8F8;
	padding: 20px;
	width: auto;
	position: absolute;
	border-radius: 10px;
	box-shadow: 5px 5px 5px rgba(0, 0, 0, 0.2);
	right: -4px;
	color: #000000;
	font-size: 13px;
	line-height: 1.4;
	opacity: 0.97;
}
.message-box-content{
	display: block;
	position: absolute;
	z-index: 999;
	top: 0;
	left: 430px;
	border: 1px solid lightgray;
	width: 320px;
	padding: 10px 10px;
	overflow-y: scroll;
	height:220px;
	background-clip: padding-box;
	box-shadow: 0 6px 12px rgba(229, 229, 229, 0.09);
	background-color: #EFEFEF;
}
.message-box-content:focus{
