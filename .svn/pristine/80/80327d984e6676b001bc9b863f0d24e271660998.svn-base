.btn { display: block; position: relative;background: #aaa; padding: 5px; float: left;text-decoration: none; cursor: pointer; }
.btn * { font-style: normal; background-image: url(/static/images/btn2.png); background-repeat: no-repeat; display: block; position: relative; }
.btn i { background-position: top left; position: absolute; margin-bottom: -5px; top: 0; left: 0; width: 5px; height: 5px; }
.btn span { background-position: bottom left; left: -5px; padding: 0 0 5px 10px; margin-bottom: -5px; }
.btn span i { background-position: bottom right; margin-bottom: 0; position: absolute; left: 100%; width: 10px; height: 100%; top: 0; }
.btn span span { background-position: top right; position: absolute; right: -10px; margin-left: 10px; top: -5px; height: 0; }

.cols{ margin:0px auto;width:780px;}
.cols li{ float:left;
	width:100px;margin:10px 10px 0 0;
	list-style:none;
	height:27px;

	line-height:25px;
	white-space:nowrap;
	overflow:hidden;
}
.cols li a{ display:block;
	height:25px;
	border:#ccc 1px solid;
	background:#f9f9f9;
	text-align:center;
}
.cols li a:hover{  border:#924 1px solid;}
.tools{ width:99%;min-width:780px;border:#924 1px solid;margin:0 auto;padding:10px;font-size:14px;}
/*message*/
.tb_message,.tb_error{padding:10px;color:red;width:600px;margin:0 auto;font-size:16px;}
.tb_message{color:green;}
.tb_message li,.tb_error li{margin:5px 0;}
.tb_btn{text-align:center;}
.tb_btn a{BACKGROUND-COLOR: #F9FAFA;border-top:#A4A4A4 1px solid;border-left:#A4A4A4 1px solid;border-right:#A4A4A4 2px solid;border-bottom:#A4A4A4 2px solid;COLOR: #495D66;padding:4px 6px;TEXT-DECORATION: none;cursor: pointer; font-size:14px;}

.win,.win a:link,.win a:visited{color:red;}
.lose,.lose a:link,.lose a:visited{color:green;}
/*sport*/
#sportResult div{ border-top:1px dotted #0099FF; }
.btype{color:red;}
.rate{color:green;}
/*result*/
.result{ width:98%;margin:0 auto;}
.result .league{line-height: 20px; font-size:14px;padding-left:5px;background-color:#3366cc}
.result td{border-color:#C3C3C3; line-height: 20px; padding:3px 2px;}
.result .end{background-color:#dcdcdc; text-align:center;}
/*search*/
#search {list-style:none;    border-bottom: 1px solid #0099FF;}
#search li{float:left;padding-right:5px;}


.ui-timepicker-div .ui-widget-header { margin-bottom: 8px;}
.ui-timepicker-div dl { text-align: left; }
.ui-timepicker-div dl dt { height: 25px; margin-bottom: -25px; }
.ui-timepicker-div dl dd { margin: 0 10px 10px 65px; }
.ui-timepicker-div td { font-size: 90%; }
.ui-tpicker-grid-label { background: none; border: none; margin: 0; padding: 0; }
.ui_tpicker_hour_label,.ui_tpicker_minute_label,.ui_tpicker_second_label,.ui_tpicker_millisec_label,.ui_tpicker_time_label{padding-left:20px}

.mutiCheckbox{padding:4px;background-color:#DDD;padding-top: 3px;}

#sysConfig input,#sysConfig textarea{ width:650px;}
#sysConfig .iButton{width:80px;}
#tjhy li{float:left;width:170px;overflow:hidden;height:30px;list-style-position: inside;}
.form .rowF .line_select_label{float:left;padding:4px 0px 4px 4px;margin:4px 0px 4px 4px;width:116px;border:1px solid #DDD;}

